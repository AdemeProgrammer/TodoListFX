<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appli.accueil.AjoutListeController">

    <children>
        <Label layoutX="70.0" layoutY="20.0" text="Créer une nouvelle liste">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label layoutX="40.0" layoutY="80.0" text="Nom de la liste :" />
        <TextField fx:id="champNomListe" layoutX="150.0" layoutY="75.0" prefWidth="200.0" />
        <Button fx:id="btnCreer" layoutX="231.0" layoutY="112.0" mnemonicParsing="false" onAction="#onCreerListeClick" text="Créer" />
        <Button fx:id="btnAnnuler" layoutX="200.0" layoutY="150.0" mnemonicParsing="false" onAction="#onAnnulerClick" text="Retour à l'accueil" />
    </children>
</AnchorPane>
