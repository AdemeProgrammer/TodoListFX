<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appli.accueil.GestionListeController">

    <children>
        <Label layoutX="14.0" layoutY="14.0" prefHeight="34.0" prefWidth="473.0" text="Gestion des listes de tâches de l'utilisateur">
            <font>
                <Font size="23.0" />
            </font>
        </Label>

        <TableView fx:id="tableauListe" layoutX="14.0" layoutY="55.0" onMouseClicked="#cliqueTableauEvent" prefHeight="300.0" prefWidth="420.0" />

        <Button fx:id="Deconnexion" layoutX="440.0" layoutY="291.0" mnemonicParsing="false" onAction="#onDeconnexionButtonClick" text="Déconnexion" />

        <Button fx:id="Accueil" layoutX="440.0" layoutY="257.0" mnemonicParsing="false" onAction="#onAccueilButtonClick" text="Accueil" />

        <Button fx:id="Supprimer" disable="true" layoutX="440.0" layoutY="329.0" mnemonicParsing="false" onAction="#onSupprimerButtonClick" text="Supprimer" />
    </children>
</AnchorPane>
